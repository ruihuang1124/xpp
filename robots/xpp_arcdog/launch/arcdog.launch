<launch>
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="arcdog_rviz_urdf_robot_description" command="$(find xacro)/xacro --inorder '$(find xpp_arcdog)/urdf/arcdog.urdf.xacro'"/>
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  -->
  <node name="urdf_visualizer_arcdog" pkg="xpp_arcdog" type="urdf_visualizer_arcdog" output="screen"/>
     
</launch>
